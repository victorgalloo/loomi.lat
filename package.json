{
  "name": "anthana-portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:bot": "npx ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}' scripts/run-tests.ts",
    "train": "npx ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}' scripts/train.ts",
    "train:apply": "npm run train -- --apply",
    "train:full": "npm run train -- --full",
    "train:live": "npx ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}' scripts/live-training.ts",
    "migrate": "npx ts-node -r tsconfig-paths/register --compiler-options '{\"module\":\"CommonJS\"}' scripts/run-migration.ts",
    "worker": "DOTENV_CONFIG_PATH=.env.local node -r dotenv/config packages/temporal-workers/node_modules/.bin/ts-node packages/temporal-workers/src/worker.ts",
    "worker:build": "npm run --prefix packages/temporal-workers build",
    "worker:start": "DOTENV_CONFIG_PATH=.env.local node -r dotenv/config packages/temporal-workers/dist/worker.js"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.21",
    "@ai-sdk/provider-utils": "^4.0.10",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.81.1",
    "@temporalio/client": "^1.11.0",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "@vercel/functions": "^3.3.6",
    "@vercel/speed-insights": "^1.3.1",
    "ai": "^6.0.55",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.554.0",
    "next": "^14.2.35",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.4.1",
    "resend": "^6.6.0",
    "stripe": "^20.3.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "baseline-browser-mapping": "^2.9.19",
    "dotenv": "^16.6.1",
    "pg": "^8.17.2",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
